<?xml version="1.0"?>
<robot name="bubble_man">
  
  <!-- Base link: Torso (main body) -->
  <link name="torso">
    <visual>
      <geometry>
        <sphere radius="0.50"/>
      </geometry>
      <material name="torso_color">
        <color rgba="0.8 0.4 0.2 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.50"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="35.0"/>
      <inertia ixx="2.0" ixy="0.0" ixz="0.0" iyy="2.0" iyz="0.0" izz="2.0"/>
    </inertial>
  </link>

  <!-- Head -->
